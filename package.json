{
  "name": "wifi-qr",
  "version": "1.0.0",
  "description": "Generate QR code for wifi connection",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "app:install": "npm i && npm i -g nodemon",
    "start:dev": "npm run style && npm start",
    "start": "cross-env NODE_ENV=development nodemon server.js",
    "style": "stylus ./public/  -o ./public",
    "style:watch": "stylus -w  ./public/  -o ./public"
  },
  "author": "Paniconi Fabio",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "wifi-qr-code-generator": "^1.1.1"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.21",
    "stylus": "^0.59.0"
  }
}
