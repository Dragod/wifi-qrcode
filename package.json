{
  "name": "wifi-qr",
  "version": "1.0.0",
  "description": "Generate QR code for wifi connection",
  "main": "server.js",
  "type": "module",
  "scripts": {
    "app:install": "npm i && npm i -g nodemon",
    "start:dev": "npm run style && npm start",
    "start": "cross-env NODE_ENV=development nodemon server.js",
    "style": "stylus ./public/main.styl  -o ./public",
    "style:watch": "stylus -w  ./public/main.styl  -o ./public"
  },
  "author": "Paniconi Fabio",
  "license": "ISC",
  "devDependencies": {
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.1",
    "wifi-qr-code-generator": "^1.1.1"
  },
  "dependencies": {
    "express": "^4.18.1",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.18",
    "stylus": "^0.58.1"
  }
}
